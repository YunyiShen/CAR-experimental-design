res_lw_long$model <- res_mean_long$model
res_hi_long <- melt(res_hi, id.vars="sample_size", value.name = "hi", variable.name = "prior")
res_hi_long$prior <- res_mean_long$prior
res_hi_long$model <- res_mean_long$model
